{
  "name": "@barnpros/template-react",
  "version": "0.1.0",
  "private": true,
  "description": "A template repository for bootstrapping react applications",
  "repository": "https://github.com/barnpros/template-react.git",
  "author": "Nich Secord <nicholassecord@gmail.com>",
  "scripts": {
    "build": "npm run build:setup && npm run build:webpack",
    "build:setup": "rimraf dist && mkdir dist",
    "build:webpack": "node scripts/build.js",
    "ci:format": "npm run type-check && npm run prettier:write",
    "ci:lint:css": "npm run lint:css",
    "ci:lint:ts:dev": "npm run lint:ts:dev",
    "ci:lint:ts:prod": "npm run lint:ts:prod",
    "ci:login:docker": "echo $BPCTX_DOCKER_PASS | docker login --username $BPCTX_DOCKER_USER --password-stdin",
    "ci:login:git": "git config user.email $BPCTX_GIT_EMAIL && git config user.name $BPCTX_GIT_NAME",
    "ci:test:e2e": "npm run test:e2e:run -- --record --key $CYPRESS_KEY",
    "ci:test:unit": "npm run test:unit",
    "ci:version:docker": "npm run docker:build && npm run docker:push",
    "ci:version:npm": "npm run increment-version",
    "docker:build": "bash scripts/docker/build.sh",
    "docker:pull": "bash scripts/docker/pull.sh",
    "docker:push": "bash scripts/docker/push.sh",
    "docker:run": "bash scripts/docker/run.sh",
    "graphql-codegen": "graphql-codegen --config ./config/codegen.yml",
    "graphql-codegen:watch": "npm run graphql-codegen -- --watch",
    "husky:commit-msg": "commitlint -E HUSKY_GIT_PARAMS --config ./config/commitlint.config.js",
    "husky:post-merge": "yarn install",
    "husky:pre-commit": "npm run lint-staged",
    "husky:pre-push": "npm run validate-branchname && npm run validate-code",
    "increment-version": "npm run increment-version:commit && npm run increment-version:push",
    "increment-version:commit": "standard-version --releaseCommitMessageFormat \"chore(release): {{currentTag}} [ci skip]\"",
    "increment-version:push": "git push --follow-tags -u origin ${CIRCLE_BRANCH}",
    "lint-staged": "lint-staged --concurrent false --config ./config/lint-staged.config.js",
    "lint-staged:eslint": "eslint --fix --cache --format=pretty --cache-location config/eslint/.eslintcache -c config/eslint/dev.config.js",
    "lint-staged:imports": "import-sort --write",
    "lint-staged:jest": "npm run test:unit -- --findRelatedTests",
    "lint-staged:package-json": "sort-package-json",
    "lint-staged:prettier": "npm run prettier -- --write",
    "lint-staged:stylelint": "stylelint --config config/stylelint/.stylelintrc.js --cache --cache-location ./config/stylelint/.stylelintcache",
    "lint:css": "stylelint \"src/**/*.tsx\" --config config/stylelint/.stylelintrc.js --cache --cache-location ./config/stylelint/.stylelintcache",
    "lint:ts": "eslint src/**/* --ext .ts,.tsx --fix --cache --cache-location config/eslint/.eslintcache --format=pretty",
    "lint:ts:dev": "yarn lint:ts -c config/eslint/dev.config.js",
    "lint:ts:prod": "yarn lint:ts -c config/eslint/prod.config.js",
    "prettier": "prettier --config ./config/prettier/prettier.config.js --ignore-path ./config/prettier/.prettierignore",
    "prettier:write": "npm run prettier -- --write src/**/*",
    "start:dev": "node scripts/start.js",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run",
    "test:unit": "jest --bail --config config/jest/jest.config.js",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "type-check": "tsc",
    "validate-branchname": "npx branch-name-lint branch-name-lint.config.json",
    "validate-code": "npm run lint:ts:dev && npm run lint:css && npm run test:unit:coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@barnpros/eslint-config": "^6.0.0",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@graphql-codegen/cli": "^1.16.3",
    "@graphql-codegen/typescript": "^1.16.3",
    "@graphql-codegen/typescript-operations": "^1.16.3",
    "@graphql-codegen/typescript-react-apollo": "^1.16.3",
    "@svgr/webpack": "4.3.3",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/testing-library__cypress": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "branch-name-lint": "^1.4.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "cypress": "^4.10.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^7.4.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^4.0.6",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "husky": "^4.2.5",
    "identity-obj-proxy": "3.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-renke": "^6.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "lint-staged": "^10.2.11",
    "pnp-webpack-plugin": "1.6.4",
    "prettier": "^2.0.5",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.44.0",
    "standard-version": "^8.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-pnp": "1.1.6",
    "typescript": "~3.7.2",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  }
}
